{
    "type": "proxy|static",
    "header": {
        "Custom-Header": "Response"
    },
    
    "proxy": {
        "server ": ["127.0.0.1", "127.0.0.2"],
        "cache": {
            "arg": true,
            "method": ["get","post"],
            "expire": "2025-02-05 11:15:00"
        },
        "header": {
            "Custom-Header": "Request"
        }
    },
    "static": {
        "root": "/usr/share/nginx/html"
    }
}