{
    "site": 1,
    "type": "proxy",
    "security": {
        "limit": {
            "status": false,
            "rate": 300,
            "queries": 2,
            "seconds": 2
        },
        "sign": {
            "status": false,
            "method": "HMAC_MD5",
            "key": "wafcdn",
            "expireargs": "t",
            "signargs": "s"
        }
    },
    "static": {
        "root": "/usr/share/nginx/html"
    },
    "proxy": {
        "server": "http://127.0.0.1:8000",
        "host": "www.baidu.com",
        "cache": {
            "arg": true,
            "method": ["get","post"],
            "expire": "2025-02-05 11:15:00"
        },
        "header": {
            "Custom-Header": "Request"
        }
    },
    "header": {
        "Custom-Header": "Response"
    }
}