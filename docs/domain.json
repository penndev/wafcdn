{
    "site": 1,
    "type": "proxy|static",

    "security": {
        "limit": {
            "status": true,
            "rate": 300,
            "queries": 2,
            "seconds": 2
        },
        "sign": {
            "status": true,
            "key": "wafcdn",
            "method": "HMACMD5",
            "timeargs": "t",
            "signargs": "s",
            "expires": 150
        }
    },
    "header": {
        "Custom-Header": "Response"
    },
    "proxy": {
        "server ": ["127.0.0.1", "127.0.0.2"],
        "cache": {
            "arg": true,
            "method": ["get","post"],
            "expire": "2025-02-05 11:15:00"
        },
        "header": {
            "Custom-Header": "Request"
        }
    },
    "static": {
        "root": "/usr/share/nginx/html"
    }
}